{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#89B4FA",
    "green": "#A5F69C",
    "lavender": "#B4BEFE",
    "os": "#ACB0BE",
    "pink": "#F5C2E7",
    "text": "#494D64",
    "white": "#FFFFFF"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{.Icon}} ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b4",
          "background": "p:blue",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .UserName }}@{{ .HostName }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:blue",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "template": " {{ .Path }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:pink",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": " {{ .HEAD }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:lavender",
          "type": "git",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue235 {{ .Venv }} ",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:green",
          "type": "python",
          "style": "powerline"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}